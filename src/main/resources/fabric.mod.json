{
	"schemaVersion": 1,
	"id": "datquarry",
	"version": "${version}",
	"name": "DatQuarry",
	"description": "A Absolute insane stupid Quarry Mod",
	"authors": [
		"VersusTuneZ"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/datquarry/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "dev.vstz.DatQuarry",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "dev.vstz.DatQuarryModClient",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "dev.vstz.DatQuarryDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"datquarry.mixins.json",
		{
			"config": "datquarry.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.19.2",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": ">=1.8.21"
	},
	"suggests": {
		"another-mod": "*"
	}
}